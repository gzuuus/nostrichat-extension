{
    "name": "Nostri-everywhere",
    "version": "1.0",
    "description": "Loads nostri.chat everywhere",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://nostri.chat; object-src 'self'",
    "manifest_version": 2
  }
  