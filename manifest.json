{
    "name": "Nostri-everywhere",
    "version": "1.0",
    "description": "Loads nostri.chat everywhere",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    }
  }
  